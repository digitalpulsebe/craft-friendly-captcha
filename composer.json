{
    "name": "digitalpulsebe/craft-friendly-captcha",
    "description": "Integrate Friendly Captcha to fight spam in your Craft CMS forms",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "friendly captcha"
    ],
    "support": {
        "docs": "https://github.com/digitalpulsebe/craft-friendly-captcha/blob/master/README.md",
        "issues": "https://github.com/digitalpulsebe/craft-friendly-captcha/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Pulse",
            "homepage": "https://www.digitalpulse.be/"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
          "digitalpulsebe\\friendlycaptcha\\": "src/"
        }
    },
    "extra": {
        "name": "Friendly Captcha",
        "handle": "friendly-captcha",
        "developer": "Digital Pulse",
        "developerUrl": "https://www.digitalpulse.be/",
        "documentationUrl": "https://github.com/digitalpulsebe/craft-friendly-captcha/blob/master/README.md",
        "changelogUrl": "https://github.com/digitalpulsebe/craft-friendly-captcha/blob/master/CHANGELOG.md",
        "components": {
            "validate": "digitalpulsebe\\friendlycaptcha\\services\\ValidateService"
        },
        "class": "digitalpulsebe\\friendlycaptcha\\FriendlyCaptcha"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
